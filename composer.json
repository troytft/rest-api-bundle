{
    "name": "troytft/rest-api-bundle",
    "type": "library",
    "description": "REST API Bundle is abstraction layer for requests and responses",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "RestApiBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/src"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "devizzent/cebe-php-openapi": "^1.0",
        "doctrine/annotations": "^1.13",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/orm": "^2.9",
        "phpdocumentor/reflection-docblock": "^5.2",
        "symfony/cache": "^5.3",
        "symfony/expression-language": "^5.3",
        "symfony/filesystem": "^5.3",
        "symfony/finder": "^5.3",
        "symfony/framework-bundle": "^5.3",
        "symfony/http-kernel": "^5.3",
        "symfony/property-info": "^5.3",
        "symfony/routing": "^5.3",
        "symfony/serializer": "^5.3",
        "symfony/translation": "^5.3",
        "symfony/validator": "^5.3",
        "symfony/yaml": "^5.3"
    },
    "bin": ["bin/generate-docs"],
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.75",
        "nyholm/symfony-bundle-test": "^2.0",
        "phpbench/phpbench": "^1.0.4",
        "phpstan/phpstan": "~2.1.16",
        "phpunit/phpunit": "~10.5.46",
        "slevomat/coding-standard": "~8.18.0",
        "spatie/phpunit-snapshot-assertions": "~5.2.0",
        "squizlabs/php_codesniffer": "~3.13.0"
    }
}
