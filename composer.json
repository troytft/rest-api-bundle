{
    "name": "troytft/rest-api-bundle",
    "type": "library",
    "description": "REST API Bundle is abstraction layer for requests and responses",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "RestApiBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/src",
            "TestApp\\": "tests/test-app"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "cebe/php-openapi": "^1.5",
        "doctrine/annotations": "^1.13",
        "doctrine/doctrine-bundle": "^1.12 || ^2.3",
        "doctrine/orm": "^2.5",
        "phpdocumentor/reflection-docblock": "^5.2",
        "symfony/expression-language": "^4.4 || ^5.3",
        "symfony/filesystem": "^4.4 || ^5.3",
        "symfony/finder": "^4.4 || ^5.3",
        "symfony/framework-bundle": "^4.4 || ^5.3",
        "symfony/http-kernel": "^4.4 || ^5.3",
        "symfony/routing": "^4.4 || ^5.3",
        "symfony/serializer": "^4.4 || ^5.3",
        "symfony/translation": "^4.4 || ^5.3",
        "symfony/validator": "^4.4 || ^5.3",
        "symfony/yaml": "^4.4 || ^5.3",
        "symfony/property-info": "^4.4 || ^5.3",
        "symfony/polyfill-php81": "^1.23"
    },
    "bin": ["bin/generate-docs"],
    "require-dev": {
        "nyholm/symfony-bundle-test": "^1.7",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.5",
        "slevomat/coding-standard": "^6.4",
        "squizlabs/php_codesniffer": "^3.6",
        "spatie/phpunit-snapshot-assertions":"^4.2",
        "phpbench/phpbench": "^1.0.2"
    }
}
