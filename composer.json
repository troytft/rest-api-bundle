{
    "name": "troytft/rest-api-bundle",
    "type": "library",
    "description": "REST API Bundle is abstraction layer for requests and responses",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "RestApiBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/src",
            "TestApp\\": "tests/test-app"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.2.5",
        "ext-json": "*",
        "cebe/php-openapi": "~1.0",
        "doctrine/annotations": "~1.6",
        "doctrine/doctrine-bundle": "^1.0 || ^2.0",
        "doctrine/orm": "~2.5",
        "phpdocumentor/reflection-docblock": "^5.2",
        "symfony/expression-language": "^4.0 || ^5.0",
        "symfony/filesystem": "^4.0 || ^5.0",
        "symfony/finder": "^4.0 || ^5.0",
        "symfony/framework-bundle": "^4.0 || ^5.0",
        "symfony/http-kernel": "^4.0 || ^5.0",
        "symfony/polyfill-php73": "^1.12",
        "symfony/routing": "^4.0 || ^5.0",
        "symfony/serializer": "^4.0 || ^5.0",
        "symfony/translation": "^4.0 || ^5.0",
        "symfony/validator": "^4.0 || ^5.0",
        "symfony/yaml": "^4.0 || ^5.0",
        "troytft/mapper": "~1.6.0"
    },
    "suggest": {
        "ext-zlib": "For support gzip requests"
    },
    "bin": ["bin/generate-docs"],
    "require-dev": {
        "nyholm/symfony-bundle-test": "dev-master",
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^8.5",
        "slevomat/coding-standard": "^6.4",
        "squizlabs/php_codesniffer": "^3.5"
    }
}
