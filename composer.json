{
    "name": "troytft/rest-api-bundle",
    "type": "library",
    "description": "REST API Bundle is abstraction layer for requests and responses",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "RestApiBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "doctrine/orm": "~2.5",
        "symfony/framework-bundle": "~3.4",
        "symfony/http-kernel": "~3.4",
        "symfony/translation": "~3.4",
        "symfony/validator": "~3.3",
        "troytft/mapper": "~1.3.0"
    },
    "suggest": {
        "ext-zlib": "For support gzip requests"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "~1.11",
        "nyholm/symfony-bundle-test": "dev-master",
        "phpstan/phpstan": "^0.11.15",
        "phpunit/phpunit": "~8.1.2",
        "slevomat/coding-standard": "~5.0",
        "squizlabs/php_codesniffer": "~3.4.2"
    }
}
